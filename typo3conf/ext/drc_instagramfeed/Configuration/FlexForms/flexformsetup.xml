<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Instagram Settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.token>
                        <TCEforms>
                            <label>Access Token Key </label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config> 
                        </TCEforms>
                    </settings.token>
                    <settings.clientId>
                        <TCEforms>
                            <label>Client ID</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                            </config> 
                        </TCEforms>
                    </settings.clientId>

                    <settings.limit>
                        <TCEforms>
                            <label>Limit Items(Default : 10 items)</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                            </config> 
                        </TCEforms>
                    </settings.limit>

                    <settings.row>
                        <TCEforms>
                            <label>Bootstrap Grid Layouts( Number of Columns)</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                <numindex index="0" type="array">
                                <numindex index="0">Single Columns</numindex>
                                <numindex index="1">12</numindex>
                                </numindex>    
                                <numindex index="1" type="array">
                                <numindex index="0">Two Columns</numindex>
                                <numindex index="1">6</numindex>
                                </numindex>
                                <numindex index="2" type="array">
                                <numindex index="0">Three Columns</numindex>
                                <numindex index="1">4</numindex>
                                </numindex>
                                <numindex index="3" type="array">
                                <numindex index="0">Four Columns</numindex>
                                <numindex index="1">3</numindex>
                                </numindex>
                                <numindex index="4" type="array">
                                <numindex index="0">Six Columns</numindex>
                                <numindex index="1">2</numindex>
                                </numindex>
                                </items>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>  
                        </TCEforms>
                    </settings.row>

                    <settings.shape>
                        <TCEforms>
                            <label>Shapes of Thumbnail Images</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                <numindex index="0" type="array">
                                <numindex index="0">Square</numindex>
                                <numindex index="1">img-thumbnail</numindex>
                                </numindex>
                                <numindex index="1" type="array">
                                <numindex index="0">Circle</numindex>
                                <numindex index="1">img-circle img-thumbnail</numindex>
                                </numindex>
                                <numindex index="2" type="array">
                                <numindex index="0">Rounded</numindex>
                                <numindex index="1">img-rounded</numindex>
                                </numindex>
                                </items>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>  
                        </TCEforms>
                    </settings.shape>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>